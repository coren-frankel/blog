{
  "name": "law-blog",
  "version": "0.1.0",
  "description": "Strapi TanStack Start blog app",
  "private": true,
  "scripts": {
    "client": "pnpm dev --prefix ../client/",
    "server": "pnpm dev --prefix ../server/",
    "setup:client": "cd client && pnpm && node ../copy-env.mjs ./",
    "setup:server": "cd server && pnpm && node ../copy-env.mjs ./",
    "setup": "pnpm install && pnpm setup:client && pnpm setup:server",
    "dev": "pnpm concurrently \"cd server && pnpm develop\" \"pnpm wait-on http://localhost:1337 && cd client && pnpm dev\"",
    "seed": "cd server && pnpm strapi import -f ../seed-data.tar.gz --force",
    "export": "cd server && pnpm strapi export --no-encrypt -f ../seed-data"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "~1.20.1",
    "@types/node": "^25.0.6",
    "concurrently": "^9.2.1",
    "typescript": "^5.0.0",
    "wait-on": "^9.0.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.0"
}
